// 1.Write a program to print your name in Dart.
void main(){
  var name = 'Suresh';
  print ("My name is $name");
}

// 2.Write a program to print Hello I am “John Doe” and Hello I’am “John Doe” with single and double quotes.
void main(){
  String name= '"John Doe"';
  print ('Hello I am $name ');
  print ("Hello I'm $name ");
}

// 3.Declare constant type** of int set value 7.
void main(){
  const int num1 =7;
  print (num1);
}

// 4.Write a program in Dart that finds simple interest.
import 'dart:io';

void main(){
  print ("Enter the Principal\n");
  int principal= int.parse(stdin.readLineSync()!);
  print ("Enter the Time\n");
  int time = int.parse(stdin.readLineSync()!);
  print ("Enter the Rate\n");
  int Rate = int.parse(stdin.readLineSync()!);
  double SI = (principal*Rate*time)/100;
  print("\nThe Simple Interest is $SI\n");
}
// 5. Write a program to print a square of a number using user input.

import 'dart:io';

void main(){
  print("Enter the Number");
  int num1 = int.parse(stdin.readLineSync()!);
  int result = num1 * num1;
  print("The Square of input is $result");
}
// 6. Write a program to print full name of a from first name and last name using user input.
import 'dart:io';
void main()
{
  print("Enter the first name");
  String fName = stdin.readLineSync()!; 
  print("Enter the second name");
  String sName = stdin.readLineSync()!;
  print("The full name of the person is $fName $sName");
}  
// 7. Write a program to find quotient and remainder of two integers.
import 'dart:io';
 void main()
 {
  print("Enter the first number");
  int num1 = int.parse(stdin.readLineSync()!);
  print("Enter the second number");
  int num2 = int.parse(stdin.readLineSync()!);
  double quotient = num1/num2;
  int remainder = num1%num2;
  print("""The quotient is $quotient
        The remainder is $remainder""");
 }

// 8. Write a program to swap two numbers.
import 'dart:io';
void main (){
  print ("Enter first number");
  double num1 = double.parse(stdin.readLineSync()!);
  print ("Enter second number");
  double num2 = double.parse(stdin.readLineSync()!);
  print("Before swapping values in 1 is $num1, 2 is $num2 ");
  List temp = [num1, num2];
  num1 = temp[1];
  num2 = temp [0];
  print("After Swaping values in 1 is $num1,2 is $num2");

}
// 9. Write a program to remove all the whiteSpaces from string.
import 'dart:io';
void main (){
  print ("Enter the String");
  String value = stdin.readLineSync()!;
  print ("Before trimming $value ");
  print ("After Trimming ${value.trim()}");
}
// 10. Write a dart program to convert String to int.
import 'dart:io';
void main(){
  print ("Enter the variable in String");
  String value = stdin.readLineSync()!;
  int val = int.parse(value);
  print("The value in Integer is $val");
}
/* 11. Suppose, you often go to restaurant with
 friends and you have to split amount of bill. Write a program to calculate
 split amount of bill. Formula= (total bill amount) / number of people*/
 import 'dart:io';

void main()
 {
  print ("Enter the number of People");
  int num1 = int.parse(stdin.readLineSync()!);
  print ("Enter the amount");
  double amount = double.parse(stdin.readLineSync()!);
  double result = amount/num1;
  print("The Equal amount of money is $result");
 }
